        <parameter name="weight" value="123456"/>
        <parameter name="name" value="AA"/>
        <classes>
            <class name="demo.DataDriveTestingDemo">
                <methods>
                    <include name="testWithParameters"/>
                </methods>
            </class>
        </classes>
    </test>

    <test verbose="2" preserve-order="true" name="DataDriveTest 1">
        <parameter name="weight" value="32"/>
        <parameter name="name" value="B"/>
        <classes>
            <class name="demo.DataDriveTestingDemo">
                <methods>
                    <include name="testWithParameters"/>
                    <include name="testWithData"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>